{
  "name": "@shadowagent/sdk",
  "version": "0.1.0",
  "description": "ShadowAgent SDK - Privacy-Preserving AI Agent Marketplace on Aleo",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./agent": {
      "types": "./dist/agent.d.ts",
      "import": "./dist/agent.mjs",
      "require": "./dist/agent.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts src/client.ts src/agent.ts --format cjs,esm --dts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@provablehq/sdk": "^0.9.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "aleo",
    "shadowagent",
    "ai",
    "marketplace",
    "privacy",
    "zk",
    "zero-knowledge",
    "x402",
    "payments",
    "escrow"
  ],
  "author": "ShadowAgent Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shadowagent/sdk"
  },
  "bugs": {
    "url": "https://github.com/shadowagent/sdk/issues"
  },
  "homepage": "https://shadowagent.dev"
}
